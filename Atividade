#include <stdio.h>

int main() {
  char nome[20];
  int idade[5];
  int matricula[20];
  char endereço[50];
  char curso[20];
  char periodo[20];
  char disciplinas[50];

  printf("Digite seu Nome: ");
  scanf(" %s ", &nome);
  printf("Digite sua Idade: ");
  scanf(" %d ", &idade);
  printf("Digite seu Número de Matricula: ");
  scanf(" %d ", &matricula);
  printf("Digite seu Endereço: ");
  scanf(" %s ", &endereço);
  printf("Digite seu Curso: ");
  scanf(" %s ", &curso);
  printf("Digite o Período de suas Aulas: ");
  scanf(" %s ", &periodo);
  printf("Digite quais as suas Disciplinas: ");
  scanf(" %s ", &disciplinas);

  printf("Nome: %s\n", nome);
  printf("Idade: %d\n", idade);
  printf("Numero de Matrícula: %d\n", matricula);
  printf("Endereço: %s\n", endereço);
  printf("Curso: %s\n", curso);
  printf("Periodo: %s\n", periodo);
  printf("Disciplinas: %s\n", disciplinas);

  return 0;
}
